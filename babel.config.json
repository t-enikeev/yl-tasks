{
  "presets": ["@babel/env", "@babel/react"],
  "env": {
    "development": {
      "plugins": [
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
        "@babel/plugin-transform-runtime"
      ]
    },
    "production": {
      "plugins": [
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
        "@babel/plugin-transform-runtime"
      ]
    }
  }
}
